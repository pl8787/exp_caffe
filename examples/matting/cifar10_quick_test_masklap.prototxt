name: "CIFAR10_quick_test"

layers {
  name: "trimap"
  type: IMAGE_DATA
  top: "trimap"
  top: "l2"
  image_data_param {
    source: "list_trimap_masklap.txt"
    batch_size: 1
    scale: 0.0078125
    bias: 128
    channels: 1
  }
}

layers {
  name: "gt"
  type: IMAGE_DATA
  top: "gt"
  top: "l3"
  image_data_param {
    source: "list_gt_masklap.txt"
    batch_size: 1
    scale: 0.0078125
    bias: 128
    channels: 1
  }
}

layers {
  name: "cccp0"
  type: MASK_WEIGHT
  bottom: "trimap"
  bottom: "trimap"
  top: "cccp0"
  mask_weight_param {
    weight_filler {
      type: "gaussian"
      std: 0.1
    }
    need_mask: true
    need_bottom: false
    expand0: 2
  }
}

layers {
  name: "tanh1"
  type: TANH
  bottom: "cccp0"
  top: "cccp0"
}

layers {
  name: "loss"
  type: MATTING
  bottom: "cccp0"
  bottom: "gt"
  bottom: "trimap"
  top: "loss"
}
